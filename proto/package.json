{
  "name": "aicode-review-protos",
  "version": "1.0.0",
  "description": "gRPC Protocol Buffers for AI Code Review Assistant",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:python",
    "build:js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generated/js --grpc_out=grpc_js:./generated/js --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I . analysis.proto github.proto",
    "build:python": "python -m grpc_tools.protoc -I. --python_out=./generated/python --grpc_python_out=./generated/python analysis.proto github.proto",
    "clean": "rm -rf generated"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "grpc-tools": "^1.12.4"
  }
}
